// Calculate performance metrics for each preset
f_calculate_preset_metrics(trades_array, equity) =>
    total_trades = array.size(trades_array)
    if total_trades == 0
        result = array.new<float>(5)
        array.set(result, 0, float(na))
        array.set(result, 1, float(na))
        array.set(result, 2, float(na))
        array.set(result, 3, float(na))
        array.set(result, 4, float(na))
        result
    else
        winning_trades = 0
        total_profit = 0.0
        total_loss = 0.0
        
        for i = 0 to total_trades - 1
            trade_result = array.get(trades_array, i)
            total_profit := total_profit + math.max(trade_result, 0)
            total_loss := total_loss + math.min(trade_result, 0)
            if trade_result > 0
                winning_trades := winning_trades + 1
        
        win_rate = total_trades > 0 ? (winning_trades / total_trades) * 100 : na
        profit_factor = math.abs(total_loss) > 0 ? total_profit / math.abs(total_loss) : na
        net_profit_pct = (equity - 10000) / 10000 * 100
        avg_trade = total_trades > 0 ? net_profit_pct / total_trades : na
        max_dd_pct = float(na)  // Simplified for now
        
        result = array.new<float>(5)
        array.set(result, 0, win_rate)
        array.set(result, 1, profit_factor)
        array.set(result, 2, max_dd_pct)
        array.set(result, 3, net_profit_pct)
        array.set(result, 4, avg_trade)
        result

